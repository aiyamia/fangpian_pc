<Window x:Class="fangpian_pc.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:fangpian_pc"
        mc:Ignorable="d"
        Title="MainWindow" Height="150" Width="300" ShowInTaskbar="False" Visibility="Hidden">
    <!---->
    <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
        <Grid Margin="60,5">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto"></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Label Content="control = " FontSize="16"></Label>
            <TextBlock Name="controlText" Grid.Column="1" Text="false" VerticalAlignment="Center" Foreground="#ff5151" FontSize="16"></TextBlock>
        </Grid>
        <Grid Margin="60,5">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto"></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Label Content="alt = " FontSize="16"></Label>
            <TextBlock Name="altText" Grid.Column="1" Text="false" VerticalAlignment="Center" Foreground="#ff5151" FontSize="16"></TextBlock>
        </Grid>
    </StackPanel>
</Window>
